{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "import/no-import-module-exports": "off",
    "import/no-dynamic-require": "off",
    "no-param-reassign": "off",
    "no-console": "off",
    "import/no-relative-packages": "off"
  }
}
